current_users = ['mom', 'dad', 'sister', 'grandma', 'grandpa']
new_users = ['Mom', 'aunt', 'uncle', 'cousin', 'nephew']

for new_user in new_users:
    if new_user.upper() in (name.upper() for name in current_users):
        print(f"{new_user} already exists. Please enter a new one.")
    else:
        print(f"{new_user} is available!")
